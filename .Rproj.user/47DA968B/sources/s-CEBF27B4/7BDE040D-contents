#!/bin/R_Script

packs <- c("RColorBrewer", "gplots", "statmod", "pheatmap", "rgl", "amap", "e107", "scales", "cluster", "bios2md", "cowplot", "ggplot2", "jsonlite", "tidyverse")                
                
install.packages(packs)                
                

# Instalar bioconductor biocLite                
source("https://bioconductor.org/biocLite.R")
biopacks <- c("DESeq2", "edgeR", "biomaRt", "Rsamtools", "readr", "org.Hs.eg.db", "GO.db", "GenomicAlignments", "Rsubread", "ShortRead", "GenomicFeatures", "clusterProfiler")

biocLite(biopacks)

